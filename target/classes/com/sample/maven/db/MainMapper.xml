<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MainMapper">
	
	<select id="selectMainList" parameterType="java.util.HashMap" resultType="com.sample.maven.dto.empDto">
		<![CDATA[
			SELECT 
	    		MEM_SEQ as memSeq
	    		, MEM_NM as memNm
	    		, MEM_DEPT as memDept
	    	FROM 
	    		EMP
		]]>
	</select>

</mapper>